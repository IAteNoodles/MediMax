models:
  - name: cardiovascular_risk
    description: "Cardiovascular disease risk prediction model"
    tool: "Predict_Cardiovascular_Risk_With_Explanation"
    parameters:
      - age
      - gender
      - height
      - weight
      - ap_hi
      - ap_lo
      - cholesterol
      - gluc
      - smoke
      - alco
      - active
    
  - name: diabetes_risk
    description: "Diabetes risk prediction model"
    tool: "Predict_Diabetes_Risk_With_Explanation"
    parameters:
      - age
      - gender
      - hypertension
      - heart_disease
      - smoking_history
      - bmi
      - HbA1c_level
      - blood_glucose_level
